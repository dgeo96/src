diff -urN -x CVS uClinux-dist-lpc_2478_stk-old/user/fbtest/fbtest.c uClinux-dist-lpc_2478_stk/user/fbtest/fbtest.c
--- uClinux-dist-lpc_2478_stk-old/user/fbtest/fbtest.c	1970-01-01 02:00:00.000000000 +0200
+++ uClinux-dist-lpc_2478_stk/user/fbtest/fbtest.c	2008-10-07 05:51:44.000000000 +0300
@@ -0,0 +1,376 @@
+/****************************************************************************
